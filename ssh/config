    HostName github.com
    ForwardAgent no
    User git
    # socks5端口号
    # ProxyCommand /usr/bin/nc -x 127.0.0.1:1086 %h %p
    IdentityFile ~/.ssh/id_rsa
    IdentitiesOnly yes

