# work
Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa

# private
Host linchuan.github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa_google

# workstation
Host 192.168.1.66        
    User         hunch
    Port         2234
    ForwardX11 yes
    ForwardX11Trusted yes
    TCPKeepAlive=yes
    ServerAliveInterval 10
    ServerAliveCountMax 5


# Azure
Host         test1 test2 azure-baidu elastic-search online-es
    Port         22
    User hunch
    ForwardX11 yes
    ForwardX11Trusted yes
    TCPKeepAlive=yes
    ServerAliveInterval 10
    ServerAliveCountMax 5
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/id_rsa_Azure

# hongkong
Host         47.224.204.109
    IdentityFile ~/.ssh/hunch_hongkong.pem
    TCPKeepAlive=yes
    ServerAliveInterval 10
    ServerAliveCountMax 5

# baidu yun
Host baidu_1
    User root
    IdentityFile ~/.ssh/id_rsa_baidu
    ForwardX11 yes
    ForwardX11Trusted yes
    ForwardAgent yes
    TCPKeepAlive=yes
    ServerAliveInterval 10
    ServerAliveCountMax 5
