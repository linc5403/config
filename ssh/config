Host huawei-xinan
    User         root
    # ForwardX11 yes
    # ForwardX11Trusted yes
    TCPKeepAlive=yes
    ServerAliveInterval 10
    ServerAliveCountMax 5
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/id_rsa


